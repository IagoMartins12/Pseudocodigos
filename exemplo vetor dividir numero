// Algoritmo que recebe 10 numeros inteiros, armazena em um vetor, divide pelo maior numero entre eles
Depois armazena o resultado no mesmo vetor. \
algoritmo vetor10Numeros 
inicio
	inteiro i, vetNumeros[10], maiorValor
	
	para (i = 0; i < 10; i++){
		escreva ("Digite um numero inteiro: ")
		leia (vetNumeros[i])
	}
	
	maiorValor = vetNumeros[0]
	para (i = 0; i < 10; i++){
		se (vetNumeros[i] > maiorValor){
			maiorValor = vetNumeros [i]
		}
	}
	
	para (i = 0; i < 10;i++){
		vetNumeros[i] = vetNumeros[i] / maiorValor
	}
	
	para (i = 0; i < 10;i++){
		escreva (vetNumeros[i])
	}
fim 
